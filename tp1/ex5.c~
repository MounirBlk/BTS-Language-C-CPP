#include<stdlib.h>
#include<stdio.h>

int main (void){

	/*Déclarer les variables*/
	int A,B;

	/*Saisir des données avec l'affichage*/
	printf("Saisir une Valeur dans la variable A ");
	scanf("%d",&A);

	printf("Saisir une Valeur dans la variable B ");
	scanf("%d",&B);
	

	/*Affichage des valeurs permutées*/
	printf("La nouvelle valeur de la variable A est : %d\n",B);
	printf("La nouvelle valeur de la variable B est : %d\n",A);

return EXIT_SUCCESS;
}

	

