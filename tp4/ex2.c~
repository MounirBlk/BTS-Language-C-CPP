#include <stdio.h>
#include <stdlib.h>
int main(void){
  /*dÃ©claration des variables*/
  int Tab[10], i, j;
  /*Remplissage du tableau*/
  for(i=1; i<=10; i++)
    {
      Tab[i]=i;
    }
  /*affichage du tableau*/
  for(j=1; j<=3; j++)
    {
      for(i=1; i<=10; i++)
	{
	  printf("%d ", Tab[i]);
	}
      printf("\n");
    }
  return EXIT_SUCCESS;
}
