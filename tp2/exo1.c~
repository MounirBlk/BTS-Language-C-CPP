#include<stdlib.h>
#include<stdio.h>

int main (void){

/*Déclarer les variables*/
int  i = 7;
char c='w';
float f= 5.5;
int a;

/*Affichage de la valeur d'expression*/
a=(i>=6) && (c=='w');
printf("La valeur relative à (i>=6) && (c=='w') est : %d \n",a);
a=(i>7) || (c==119);
printf("La valeur relative à (i>7) ||(c==119) est : %d \n",a);
a=(f<11) && (i<100);
printf("La valeur relative à (f<11) ||(i<100) est : %d \n",a);
a=(c!='p') || ((i+f)<=10);
printf("La valeur relative à (f<11) ||((i+f)=<10) est : %d \n",a);
a=(f>4.5) && (c=='a') && (i+f>12);
printf("La valeur relative à (f>4.5) && (c=='a') && (i+f>12) est : %d \n",a);
a=f*(i==7);
printf("La valeur relative à f*(i==7) est : %f \n",f);
a=i+3*(i>f);
printf("La valeur relative à i+3*(i>7) est : %d \n",a);

return EXIT_SUCCESS;
}


